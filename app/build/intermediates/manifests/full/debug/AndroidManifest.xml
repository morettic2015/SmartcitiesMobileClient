            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <!-- Defining services -->
        <service android:name="view.infoseg.morettic.com.br.infosegapp.util.MessagingService" >
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="view.infoseg.morettic.com.br.infosegapp.util.InstanceIdService" >
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <receiver
            android:name="view.infoseg.morettic.com.br.infosegapp.actions.TAPPXBroadcastReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>
